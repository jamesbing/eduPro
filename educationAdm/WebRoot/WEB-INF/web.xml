<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>generateUserServlet</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.GenerateUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>generateUserServlet</servlet-name>
    <url-pattern>/generateUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addUser</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.GetAddUserPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addUser</servlet-name>
    <url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addSuccess</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.AddSuccessPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addSuccess</servlet-name>
    <url-pattern>/addSuccess</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getUserInformation</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.GetUserInformationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getUserInformation</servlet-name>
    <url-pattern>/checkIfActived</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showMe</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.ShowMeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showMe</servlet-name>
    <url-pattern>/showMe</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>exportToExcel</servlet-name>
    <servlet-class>nankai.dym.admin.servlet.ExportToExcelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>exportToExcel</servlet-name>
    <url-pattern>/exportToExcel</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>SetCharacterEncoding</filter-name>
    <filter-class>nankai.dym.admin.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>Encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SetCharacterEncoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>