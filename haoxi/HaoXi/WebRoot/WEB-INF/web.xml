<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>startpage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
		<servlet-name>activeConfirm</servlet-name>
		<servlet-class>nk.james.haoxi.servlet.ActiveConfirmServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>activeConfirm</servlet-name>
		<url-pattern>/activeConfirm</url-pattern>
	</servlet-mapping>
	 <servlet>
		<servlet-name>active</servlet-name>
		<servlet-class>nk.james.haoxi.servlet.ActiveServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>active</servlet-name>
		<url-pattern>/active</url-pattern>
	</servlet-mapping>

	
</web-app>
